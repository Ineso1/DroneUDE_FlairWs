{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${FLAIR_ROOT}/flair-src/lib/**",
                "${FLAIR_ROOT}/flair-src/lib/FlairCore/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairFilter/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairMeta/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairSensorActuator/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairIpc/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairSimulator/src",
                "${FLAIR_ROOT}/flair-src/lib/FlairVisionFilter/src",
                "/usr/include/eigen3"
            ],
            "defines": [
                "SOURCE_DIR=\"/absolute/path/to/your/source\""
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}